{
  "name": "zergbz1988/laravel-calc",
  "description": "A package to evaluate string expression",
  "keywords": ["laravel", "laravel-calc", "laravel5-package", "PHP", "laravel5"],
  "license": "MIT",
  "homepage": "https://github.com/zergbz1988/laravel-calc",
  "authors": [
    {
      "name": "Komarov Marat",
      "email": "zergbz1988@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "laravel/framework":"~5.6"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^1.1"
  },
  "autoload-dev": {
    "classmap": ["tests/CalcTest.php"]
  },
  "autoload": {
    "psr-4": {
      "Zergbz1988\\Calc\\": "src"
    },
    "classmap": [
      "src"
    ]
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zergbz1988\\Calc\\CalcServiceProvider"
      ]
    }
  }
}
